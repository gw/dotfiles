{
    "workbench.colorTheme": "Default Light+",
    "editor.fontFamily": "Iosevka Custom",
    "editor.lineHeight": 1.33,
    "window.commandCenter": false,
    "editor.fontSize": 14,
    "workbench.quickOpen.closeOnFocusLost": false,
    "editor.cursorStyle": "block",
    "editor.cursorSurroundingLines": 5,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "editor.cursorBlinking": "solid",
    "explorer.excludeGitIgnore": true,
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "workbench.sideBar.location": "right",
    "files.insertFinalNewline": true,
    "git.autofetch": true,
    "cursor-retrieval.canAttemptGithubLogin": false,
    "editor.formatOnSave": true,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "git.useIntegratedAskPass": false,
    "git.terminalAuthentication": false,
    "terminal.integrated.scrollback": 10000,
    "cursor.chat.showSuggestedFiles": true,
    "cursor.cmdk.useThemedDiffBackground": true,
    "cursor.chat.terminalShowHoverHint": false,
    "editor.minimap.autohide": true,
    "cursor.cpp.enablePartialAccepts": true,
    "docker.containers.label": "ContainerName",
    "docker.containers.description": [
        "Status"
    ],
    "docker.containers.groupBy": "None",
    "docker.containers.sortBy": "CreatedTime",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "git.enableSmartCommit": true,
    "githubPullRequests.fileListLayout": "tree",
    "workbench.panel.defaultLocation": "right",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.launchpad.indicator.enabled": false,
    "editor.inlineSuggest.enabled": false,
    "editor.quickSuggestions": {
        "other": "off"
    },
    "editor.suggestOnTriggerCharacters": false,
    "githubPullRequests.pullBranch": "never",
    "diffEditor.ignoreTrimWhitespace": false,
    "window.zoomLevel": 0.4,
    "rust-analyzer.cfg.setTest": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*.mutable:rust": {
                "underline": false,
            }
        }
    },
    "center-editor-window.threeStateToggle": true
}
